{
  "name": "markdox",
  "description": "Markdox is a documentation generator based on Dox and Markdown with support for JavaScript, CoffeeScript and IcedCoffeeScript.",
  "version": "0.1.8",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "documentation": "bin/markdox -o doc/api.md lib/markdox.js",
    "test": "./node_modules/mocha/bin/mocha test/*-test.js",
    "examples": "node examples/example.js",
    "preinstall": "npm install ../blanket"
  },
  "keywords": [
    "documentation",
    "dox",
    "markdown"
  ],
  "engines": {
    "node": ">= 0.10.0"
  },  
  "author": "Charles Bourasseau <charles.bourasseau@gmail.com>",
  "contributors": [
    "Ethan Mick <ethan@ethanmick.com>"
  ],
  "repository": {
    "type": "git", 
    "url": "https://cbou@github.com/cbou/markdox.git"
  },
  "bin": {
    "markdox": "./bin/markdox"
  },
  "config": {
    "blanket": {
      "pattern": "lib",
      "data-cover-never": "node_modules",
      "data-cover-reporter-options": {
        "shortnames": true
      }
    },
    "travis-cov": {
      "threshold": 10
    }
  },
  "dependencies": {
    "dox": "https://github.com/visionmedia/dox/tarball/master",
    "commander": ">= 0.6.0",
    "async": ">= 0.1.18",
    "ejs": ">= 0.7.2 <2.0.0",
    "underscore": ">= 1.3.3",
    "coffee-script": "1.8.0"
  },
  "devDependencies": {
    "blanket": "*",
    "mocha": "2.1.0",
    "should": "4.6.2",
    "sinon": "1.10.3",
    "travis-cov": "0.2.5"
  }
}
